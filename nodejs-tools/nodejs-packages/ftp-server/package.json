{
  "name": "@vighnesh153/ftp-server",
  "version": "0.4.5-alpha04",
  "description": "A simple ftp server cli tool for your local files",
  "license": "MIT",
  "type": "module",
  "bin": "./dist/cli.js",
  "private": false,
  "author": {
    "name": "Vighnesh Raut",
    "url": "https://vighnesh153.com",
    "email": "me@vighnesh153.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:vighnesh153/vighnesh153-monorepo.git"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:backend": "tsup --watch",
    "dev:frontend": "vite",
    "lint": "eslint --max-warnings=0 .",
    "copy": "cpy --cwd=./src/client/dist . ../../../dist/__vighnesh153-ftp-client__/",
    "build": "tsup && vite build && npm run copy"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "archiver": "^7.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "ejs": "^3.1.9",
    "express": "^4.18.3",
    "ip": "^2.0.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.11",
    "@types/archiver": "^6.0.2",
    "@types/express": "^4.17.21",
    "@types/ip": "^1.1.3",
    "@types/morgan": "^1.9.9",
    "@types/node": "*",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@vighnesh153/tsconfig": "*",
    "@vighnesh153/utils": "*",
    "@vitejs/plugin-react": "^4.2.1",
    "cpy-cli": "^5.0.0",
    "eslint-config-vighnesh153": "*",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4"
  },
  "keywords": [
    "ftp",
    "file-transfer-protocol",
    "server",
    "serve",
    "directory",
    "local",
    "localhost",
    "network",
    "vighnesh153",
    "file",
    "transfer",
    "protocol"
  ]
}
