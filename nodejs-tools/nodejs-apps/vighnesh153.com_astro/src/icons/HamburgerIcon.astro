---
import { type HTMLAttributes } from 'astro/types';

export interface Props extends HTMLAttributes<'svg'> {}

// eslint-disable-next-line @typescript-eslint/no-unused-vars
const { viewBox, xmlns, width, ...props } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 140 100"
  width={width ?? '200'}
  stroke="currentColor"
  stroke-width="15"
  stroke-linecap="round"
  stroke-linejoin="round"
  {...props}
>
  <path class="top" d="M20,20 L120,20"></path>
  <path class="middle" d="M20,50 L120,50"></path>
  <path class="bottom" d="M20,80 L120,80"></path>
</svg>
