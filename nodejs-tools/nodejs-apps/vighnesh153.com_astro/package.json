{
  "name": "vighnesh153.com_astro",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "lint": "eslint --max-warnings=0 . && astro check",
    "install-test-deps": "npx playwright install --with-deps",
    "test:playwright:interactive": "npx playwright test --ui",
    "test:playwright": "npx playwright test",
    "test:vitest": "vitest --passWithNoTests",
    "test:only": "run-s test:vitest test:playwright",
    "test": "run-s install-test-deps test:only",
    "sst:deploy:dev": "dotenv -e ../../.env.local -- sst deploy --stage=dev",
    "sst:deploy:prod": "sst deploy --stage=prod",
    "sst:remove": "dotenv -e ../../.env.local -- sst remove"
  },
  "dependencies": {
    "@vighnesh153/utils": "*"
  },
  "devDependencies": {
    "@astrojs/prefetch": "^0.2.3",
    "@astrojs/tailwind": "^4.0.0",
    "@playwright/test": "^1.35.1",
    "@types/node": "*",
    "@vighnesh153/fake-data": "*",
    "@vighnesh153/tsconfig": "*",
    "astro": "^2.7.0",
    "astro-compress": "^1.1.47",
    "autoprefixer": "^10.4.14",
    "aws-cdk-lib": "2.85.0",
    "browserslist": "^4.21.9",
    "constructs": "10.2.60",
    "cssnano": "^6.0.1",
    "cssnano-preset-advanced": "^6.0.1",
    "csstype": "^3.1.2",
    "eslint-config-vighnesh153": "*",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "postcss-custom-media": "^9.1.5",
    "postcss-import": "^15.1.0",
    "sass": "^1.63.6",
    "sst": "^2.16.3",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "vitest": "^0.33.0"
  },
  "browserslist": [
    "defaults and supports es6-module",
    "maintained node versions"
  ],
  "author": {
    "name": "Vighnesh Raut",
    "email": "me@vighnesh153.com",
    "url": "https://vighnesh153.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:vighnesh153/vighnesh153-monorepo.git"
  }
}
