    "@vighnesh153/simple-code-to-html": "*",
    "@vighnesh153/spl": "*",
    "@vighnesh153/tools-platform-independent": "*",
    "@vighnesh153/tsx-bundler": "*",
    "astro-sst": "2.43.5",
    "nanostores": "^0.11.3",
    "solid-js": "^1.8.22",
    "svelte": "^4.2.19",
    "vighnesh153-cookies": "^1.0.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.3",
    "@astrojs/mdx": "^3.1.6",
    "@astrojs/solid-js": "^4.4.1",
    "@astrojs/svelte": "^5.7.0",
    "@playform/compress": "^0.1.1",
    "@playwright/test": "^1.47.1",
    "@solidjs/testing-library": "^0.8.9",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "*",
    "@vighnesh153/fake-data": "*",
    "@vighnesh153/tsconfig": "*",
    "@vighnesh153/types": "^0.4.7",
    "astro": "^4.15.6",
    "autoprefixer": "^10.4.20",
    "browserslist": "^4.23.3",
    "constructs": "10.3.0",
    "cssnano": "^7.0.6",
    "cssnano-preset-advanced": "^7.0.6",
    "csstype": "^3.1.3",
    "eslint-config-vighnesh153": "*",
    "jsdom": "^25.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "postcss-custom-media": "^11.0.1",
    "postcss-import": "^16.1.0",
    "sass": "^1.78.0",
    "solid-devtools": "^0.30.1",
    "sst": "^3.1.10",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  },
  "browserslist": [
    "defaults and supports es6-module",
    "maintained node versions"
