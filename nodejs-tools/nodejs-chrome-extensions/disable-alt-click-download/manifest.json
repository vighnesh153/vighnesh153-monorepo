{
  "manifest_version": 3,
  "name": "Disable alt/option + Click shortcut",
  "version": "1.0",
  "description": "Disables the alt/option + click download shortcut on Chrome.",
  "icons": {
    "16": "images/logo_16x16.png",
    "32": "images/logo_32x32.png",
    "48": "images/logo_48x48.png",
    "128": "images/logo_128x128.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "js": ["scripts/index.js"]
    }
  ]
}
