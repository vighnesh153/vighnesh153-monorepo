{
  "name": "vighnesh153-monorepo-nodejs",
  "version": "0.0.0",
  "private": true,
  "prettier": "@vighnesh153/prettier-config",
  "type": "module",
  "workspaces": [
    "nodejs-apps/*",
    "nodejs-aws-serverless-tools/*",
    "nodejs-cloudflare-tools/*",
    "nodejs-docker-tools/*",
    "nodejs-packages/*",
    "nodejs-templates/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel --no-cache",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "deploy:prod": "turbo run deploy:prod",
    "changeStarToSpecificVersion": "node changeStarToSpecificVersion.js"
  },
  "devDependencies": {
    "@vighnesh153/prettier-config": "*",
    "@vighnesh153/tsconfig": "*",
    "dotenv-cli": "^7.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "rimraf": "^5.0.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@9.5.0"
}
