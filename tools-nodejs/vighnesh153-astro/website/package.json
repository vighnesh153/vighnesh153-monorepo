    "browserslist": "^4.24.4",
    "constructs": "10.4.2",
    "cssnano": "^7.0.6",
    "cssnano-preset-advanced": "^7.0.6",
    "csstype": "^3.1.3",
    "jsdom": "^26.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.1",
    "postcss-custom-media": "^11.0.5",
    "sass": "^1.83.4",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
