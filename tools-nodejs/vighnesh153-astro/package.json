{
  "name": "vighnesh153-astro",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "astro dev --host",
    "build": "astro check && astro build",
    "lint": "astro check",
    "prefmt": "cp deno-bk.json deno.json",
    "fmt": "deno fmt",
    "postfmt": "rm -rf deno.json",
    "install-test-deps": "npx playwright install --with-deps",
    "test:playwright:interactive": "npx playwright test --ui",
    "test:playwright": "npx playwright test",
    "test:vitest:watch": "vitest --passWithNoTests",
    "test:vitest": "vitest run --passWithNoTests",
    "test:only": "run-s test:vitest test:playwright",
    "test": "run-s install-test-deps test:only",
    "sst:deploy:dev": "dotenvx run --env-file=../../.env.local -- sst deploy --stage=dev",
    "sst:deploy:prod": "dotenvx run --env-file=../../.env.local -- sst deploy --stage=prod",
    "sst:remove:dev": "dotenvx run --env-file=../../.env.local -- sst remove --stage=dev",
    "sst:remove:prod": "dotenvx run --env-file=../../.env.local -- sst remove --stage=prod"
  },
  "dependencies": {
    "@nanostores/solid": "^0.5.0",
    "@std/assert": "npm:@jsr/std__assert@^1.0.6",
    "@vighnesh153/drawing-app": "0.4.8",
    "@vighnesh153/simple-code-to-html": "0.4.8",
    "@vighnesh153/spl": "0.4.8",
    "@vighnesh153/tools": "npm:@jsr/vighnesh153__tools@^0.1.10",
    "@vighnesh153/tools-browser": "npm:@jsr/vighnesh153__tools-browser@^0.1.2",
    "@vighnesh153/tsx-bundler": "0.4.8",
    "astro-sst": "2.43.5",
    "firebase": "^11.0.1",
    "nanostores": "^0.11.3",
    "solid-js": "^1.9.2",
    "svelte": "^4.2.19"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^3.1.7",
    "@astrojs/solid-js": "^4.4.2",
    "@astrojs/svelte": "^5.7.2",
    "@dotenvx/dotenvx": "^1.21.0",
    "@playform/compress": "^0.1.4",
    "@playwright/test": "^1.48.0",
    "@solidjs/testing-library": "^0.8.10",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "*",
    "@vighnesh153/fake-data": "*",
    "@vighnesh153/tsconfig": "*",
    "astro": "^4.15.12",
    "autoprefixer": "^10.4.20",
    "browserslist": "^4.24.0",
    "constructs": "10.3.0",
    "cssnano": "^7.0.6",
    "cssnano-preset-advanced": "^7.0.6",
    "csstype": "^3.1.3",
    "jsdom": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "postcss-custom-media": "^11.0.2",
    "postcss-import": "^16.1.0",
    "sass": "^1.79.4",
    "solid-devtools": "^0.30.1",
    "sst": "^3.1.72",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3",
    "vitest": "^2.1.2"
  },
  "browserslist": [
    "defaults and supports es6-module",
    "maintained node versions"
  ],
  "author": {
    "name": "Vighnesh Raut",
