---
import { graphicsProjectsMap } from "@vighnesh153/tools-browser/graphics_programming";
import CanvasProjectLayout from "@/layouts/CanvasProjectLayout.astro";
import GridPathFinderRoot from "@/components/projects/graphics/grid-path-finder/GridPathFinderRoot.svelte";

const project = graphicsProjectsMap.gridPathFinderGame;
---

<CanvasProjectLayout project={project} type="illustration">
  <GridPathFinderRoot client:load />
</CanvasProjectLayout>

<script>
  import { logAnalyticsEvent } from "@/utils/firebase_config.ts";

  logAnalyticsEvent("visit/project/graphics", {
    graphicsProjectTitle: "grid path finder",
  });
</script>
